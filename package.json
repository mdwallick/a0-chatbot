{
  "name": "a0-chatbot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "postinstall": "npm run prisma:generate",
    "vercel-build": "prisma migrate deploy && next build",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:pull": "dotenv -e .env.local -- prisma db pull",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "prisma:truncate": "dotenv -e .env.local -- tsx prisma/scripts/clear-db.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@agentic/google-drive": "^7.6.4",
    "@ai-sdk/openai": "^1.3.17",
    "@auth0/ai-vercel": "^3.0.0",
    "@auth0/nextjs-auth0": "^4.5.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^6.8.0",
    "@radix-ui/react-avatar": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-toast": "^1.2.11",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@slack/web-api": "^7.9.1",
    "@vercel/postgres": "^0.10.0",
    "ai": "^4.3.9",
    "auth0": "^4.22.0",
    "box-node-sdk": "^3.8.0",
    "canvas": "^3.1.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.0",
    "framer-motion": "^12.8.0",
    "googleapis": "^148.0.0",
    "jsforce": "^3.8.1",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.503.0",
    "mammoth": "^1.6.0",
    "next": "15.3.1",
    "next-themes": "^0.4.6",
    "openai": "^4.98.0",
    "prisma": "^6.8.0",
    "react": "^19.0.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.8",
    "usehooks-ts": "^3.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@microsoft/microsoft-graph-types": "^2.40.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pdfjs-dist": "^2.10.377",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.19.4",
    "typescript": "^5"
  }
}
